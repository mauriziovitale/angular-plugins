{
  "npmScope": "angular-custom-builders",
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "projects": {
    "lite-serve": {
      "tags": []
    },
    "lite-serve-e2e": {
      "tags": [],
      "implicitDependencies": ["lite-serve"]
    },
    "aws-actions": {
      "tags": []
    },
    "aws-actions-e2e": {
      "tags": [],
      "implicitDependencies": ["aws-actions"]
    },
    "licenses": {
      "tags": []
    },
    "licenses-e2e": {
      "tags": [],
      "implicitDependencies": ["licenses"]
    }
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  }
}
